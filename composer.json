{
  "name": "lambirou/babiphp",
  "description": "The flexible PHP Framework",
  "type": "project",
  "license": "MIT",
  "version": "1.3.1",
  "keywords": [
    "babiphp",
    "framework",
    "php"
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "hashids/hashids": "^4.1",
    "psr/container": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "BabiPHP\\": "system/"
    },
    "files": [
      "system/helpers.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
